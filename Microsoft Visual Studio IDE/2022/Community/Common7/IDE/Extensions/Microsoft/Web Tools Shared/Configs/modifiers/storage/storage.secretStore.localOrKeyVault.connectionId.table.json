{
  "kind": "secrets.secretStore",
  "restorable": true,
  "model": {
    "secretStore": "@@secretStore@@",
    "secretName": "@@connectionId@@:tableServiceUri",
    "secretValue": "@@TableEndpoint@@",
    "secretStoreResourceId": "@@SecretStoreResourceId@@",
    "secretExtraData": {
      "valueKind": "Setting"
    },
    "applicableScope": {
      "Local": [
        "LocalSecretsFile",
        "AzureKeyVault"
      ],
      "AppService": [
        "AzureKeyVault"
      ],
      "ContainerApp": [
        "AzureKeyVault"
      ]
    }
  }
}