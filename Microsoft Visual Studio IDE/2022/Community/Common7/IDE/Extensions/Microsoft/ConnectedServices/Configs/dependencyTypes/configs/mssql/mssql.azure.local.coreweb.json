{
  "feature": {
    "id": "ef.sqlServer.azure"
  },
  "requirements": [
    "vsWebWorkload"
  ],
  "editSteps": [
    "PickInstance",
    "RemoteConnection",
    "DependencySummary"
  ],
  "secretStores": [
    "secrets.user",
    "secrets.keyVault",
    "none"
  ],
  "properties": {
    "connectionId": {
      "value": "ConnectionStrings:DatabaseConnection"
    }
  },
  "persistencePreprocessors": [
    "SecretStoreResourceId",
    "SqlAzureDevelopmentFirewallRule"
  ],
  "persistenceProperties": {
    "connectionId": {
      "isBase": true
    },
    "dynamicId": {
      "isBase": true
    },
    "ignored": {
      "isBase": true
    },
    "resourceId": {
      "parametrizer": "resourceId"
    },
    "secretStore": {},
    "serviceConnectorResourceId": {
      "parametrizer": "resourceId",
      "parametrizerArguments": "resourceGroupName=serviceConnectorResourceGroupName"
    },
    "suggestion": {
      "isBase": true
    },
    "type": {
      "isBase": true
    },
    "group": {
      "isBase": true
    }
  },
  "arm": {
    "apiVersion": "2017-10-01-preview",
    "templateGenerator": {
      "id": "export",
      "model": {
        "templatePreprocessors": [
          {
            "id": "removeUnnecessaryProperties",
            "model": {
              "properties": [
                "etag",
                "id",
                "identity",
                "properties",
                "tags"
              ]
            }
          }
        ]
      }
    }
  }
}